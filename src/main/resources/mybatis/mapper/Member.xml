<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.unisem.vms.repository.MemberRepository">
    <select id="findByUsername" resultType="kr.co.unisem.vms.entity.Member">
        SELECT member_id
            , username
            , PASSWORD
            , name
            , email
            , position
            , role
            , enabled
            , created
            , updated
         FROM mbr_member
         WHERE username = #{username}
    </select>
</mapper>